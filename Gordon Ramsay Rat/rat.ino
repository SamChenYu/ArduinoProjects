#include <MP3_Module.h>
#include <SoftwareSerial.h>
#include <Servo.h>

#define button1 4

MP3 mp3;
int trackNum = 0;

Servo servo1;
int servoPin = 5;


//switch case variables
int input;
int randomFlag = 0;
int arr[30] = {19, 5, 28, 16, 7, 25, 11, 23, 4, 27, 3, 6, 9, 12, 21, 13, 29, 2, 8, 15, 1, 20, 10, 18, 22, 26, 14, 17, 24};

void setup() {
  Serial.begin(9600);
  input = 0;
  pinMode(button1, INPUT_PULLUP);
  
  mp3.begin();
  mp3.volume(30);
  mp3.eq(POP);
  
  servo1.attach(servoPin);
  //servo1.write(0-100);
  servo1.write(0);
}

void loop() {





  
  input = !digitalRead(button1)*1;




  if(input != 0){
    delay(500);
    input = !digitalRead(button1)*2;
    
    if(input == 2) {
	trackNum = 1;
	for(int i =0; i<30; i++) {
	    play();
	    trackNum++;
	}
    } else if (input == 0) {
	


	trackNum = arr[randomFlag];
	if(randomFlag == 29) {
		randomFlag == 0;
	} else {
		randomFlag++;
	}
	play();
	}
  }


}

   
